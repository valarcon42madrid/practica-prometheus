  "templating": {
    "list": []
  },
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "type": "stat",
      "title": "P90 Duraci\u00f3n HTTP (m\u00e9todo/ruta/status)",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.90, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, method, path, status))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "P95 Duraci\u00f3n HTTP (m\u00e9todo/ruta)",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, method, path))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Promedio duraci\u00f3n HTTP (m\u00e9todo/ruta)",
